<mvc:View xmlns:layout="sap.ui.layout" controllerName="com.iherb.tm.ztmiherbpurchaseorders.controller.POtable"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:t="sap.ui.table"
    xmlns:f="sap.f"
    xmlns="sap.uxap"
    xmlns:customData="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
    xmlns:m="sap.m">
    <m:Page id="page" showHeader="false">
        <m:VBox id="_IDGenVBox">
            <fb:FilterBar id="filterbar" advancedMode="false" search="onSearch" showClearOnFB="true" clear=".onClearFilters">
                <fb:filterGroupItems>
                    <fb:FilterGroupItem id="_IDGenFilterGroupItem2" name="A" label="{i18n>PurchaseOrderLabel}" groupName="Group1" visibleInFilterBar="true">
                        <fb:control>
                            <m:MultiInput id="multipleConditions1" width="100%" showValueHelp="true" valueHelpRequest=".onMultipleConditionsVHRequested">
                            </m:MultiInput>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem id="_IDGenFilterGroupItem3" name="B" label="{i18n>packageid}" groupName="Group1" visibleInFilterBar="true">
                        <fb:control>
                            <m:MultiInput id="multipleConditions2" width="100%" showValueHelp="true" valueHelpRequest="onMultipleConditionsVHRequested">
                            </m:MultiInput>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem id="_IDGenFilterGroupItem4" name="C" label="{i18n>PickUpLabel}" groupName="Group1" visibleInFilterBar="true">
                        <fb:control>
                            <m:DatePicker id="_IDGenDatePicker" displayFormat="{MM/dd/yyyy}" width="100%">
                            </m:DatePicker>
                        </fb:control>
                    </fb:FilterGroupItem>
                    <fb:FilterGroupItem id="_IDGenFilterGroupItem1" name="D" label="{i18n>POpickuplocation}" groupName="Group1" visibleInFilterBar="true">
                        <fb:control>
                            <m:MultiComboBox id="_IDGenMultiComboBox" width="100%" items="{
												path: '/ZI_PARTNER_LOC',
												templateShareable: true
											}">
                                <core:Item id="_IDGenItem" key="{Location}" text="{LocDescr}"/>
                            </m:MultiComboBox>
                        </fb:control>
                    </fb:FilterGroupItem>
                </fb:filterGroupItems>
            </fb:FilterBar>
        </m:VBox>
        <m:content>
            <t:Table id="idPOTable" rows="{path:'POTableModel>/list', sorter:{path: 'BaseBtdId', descending:true}}" threshold="100" rowMode="Auto" showNoData="false" selectionMode="Multi" selectionBehavior="Row" alternateRowColors="true">
                <t:extension>
                    <m:OverflowToolbar id="_IDGenOverflowToolbar">
                        <m:Title id="_IDGenTitle1" text="Purchase Order ({=${POTableModel>/list}.length})" titleStyle="H3" class="sapUiTinyMarginBeginEnd"/>
                        <m:ToolbarSpacer id="_IDGenToolbarSpacer" />
                        <m:OverflowToolbarButton id="_IDGenOverflowToolbarButton" icon="sap-icon://refresh" tooltip="Refresh" press="onRefresh" />
                    </m:OverflowToolbar>
                </t:extension>
                <t:columns>
                    <t:Column id="_IDGenColumn2" width="8rem" sortProperty="BaseBtdId" filterProperty="BaseBtdId">
                        <m:Label id="_IDGenLabel2" text="{i18n>purchaseOrder}"/>
                        <t:template>
                            <m:ObjectNumber id="_IDGenText2" number="{POTableModel>BaseBtdId}"></m:ObjectNumber>
                        </t:template>
                    </t:Column>
                    <t:Column id="_IDGenColumn3" width="8rem" sortProperty="TorId" filterProperty="TorId">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabel3" text="{i18n>fuItmNum}"/>
                        <t:template>
                            <m:Text id="_IDGenText3" text="{POTableModel>TorId}"/>
                        </t:template>
                    </t:Column>
                    <t:Column id="_IDGenColumn4" width="8rem" sortProperty="ItemId" filterProperty="ItemId">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabel4" text="{i18n>fuItemNum}"/>
                        <t:template>
                            <m:Text id="_IDGenText4" text="{POTableModel>ItemId}"/>
                        </t:template>
                    </t:Column>
                    <t:Column id="_IDGenColumn5" width="8rem" sortProperty="ProductId" filterProperty="ProductId">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel5" text="{i18n>productId}"/>
                        <t:template>
                            <m:Text id="_IDGenText5" text="{POTableModel>ProductId}"/>
                        </t:template>
                    </t:Column>
                    <t:Column id="_IDGenColumn6" width="18rem" sortProperty="ItemDescr" filterProperty="ItemDescr">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabe6" text="{i18n>productDesc}"/>
                        <t:template>
                            <m:Text id="_IDGenText6" text="{POTableModel>ItemDescr}"/>
                        </t:template>
                    </t:Column>
                    <!-- PO Quantity -->
                    <t:Column id="_IDGenColumn7" width="6rem" hAlign="Center" sortProperty="QuaPcsVal" filterProperty="QuaPcsVal">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabel7" text="{i18n>quantity}"/>
                        <t:template>
                            <m:ObjectNumber id="_IDGenObjectNumber7" number="{POTableModel>QuaPcsVal}" unit="{POTableModel>QuaPcsUni}"/>
                        </t:template>
                    </t:Column>
                    <!-- PO Pick up Location -->
                    <t:Column id="_IDGenColumn8" width="18rem" sortProperty="SrcLocDescr" filterProperty="SrcLocDescr">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabel8" text="{i18n>POpickuplocation}"/>
                        <t:template>
                            <m:Text id="_IDGenText8" text="{POTableModel>SrcLocDescr}"></m:Text>
                        </t:template>
                    </t:Column>
                    <!-- PO Pickup Date (time zone maintain ) -->
                    <t:Column id="_IDGenColumn9" width="9rem" sortProperty="PickupDt" filterProperty="PickupDt">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="idLabPickup9" text="{i18n>PoPickupDt}" />
                        <t:template>
                            <m:Text id="idPoPickDt9" text="{path:'POTableModel>PickupDt',type: 'sap.ui.model.type.DateTime', formatOptions: {'pattern': 'MM/dd/yyyy HH:mm'}}"></m:Text>
                        </t:template>
                    </t:Column>
                    <!-- PO Destination Location -->
                    <t:Column id="_IDGenColumn10" width="11rem" sortProperty="DesLocDescr" filterProperty="DesLocDescr">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="idLabPickup10" text="{i18n>PoDestinLoc}" />
                        <t:template>
                            <m:Text id="idPodesloc10" text="{POTableModel>DesLocDescr}"></m:Text>
                        </t:template>
                    </t:Column>
                    <!-- Po Delivery Date -->
                    <t:Column id="_IDGenColumn11" width="9rem" sortProperty="DeliveryDt" filterProperty="DeliveryDt">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="idLabPickup11" text="{i18n>PoDeliDt}" />
                        <t:template>
                            <m:Text id="idPoDeliveryDt11" text="{path:'POTableModel>DeliveryDt',type: 'sap.ui.model.type.DateTime', formatOptions: {'pattern': 'MM/dd/yyyy HH:mm'}}"></m:Text>
                        </t:template>
                    </t:Column>
                    <!-- Confirm Quantity $$$$$$$$$$$$$$$$$$$$editttt$$$$$$$$$$$$$$$$$$$$$$$$$$$-->
                    <t:Column id="_IDGenColumn12" width="8rem" sortProperty="PkgQuaPcsVal" filterProperty="PkgQuaPcsVal">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel12" text="{i18n>confipackageQty}" required="true"/>
                        <t:template>
                            <m:HBox id="_IDGenHBox12">
                                <m:Input id="confQtyId" width="4rem" value="{POTableModel>PkgQuaPcsVal}" type="Number" required="true" editable="{= ${POTableModel>ReadOnly} !== 'X' }"/>
                                <m:Input id="confQty" value="{POTableModel>PkgQuaPcsUni}" type="Text" editable="{= ${POTableModel>ReadOnly} !== 'X' }" />
                            </m:HBox>
                        </t:template>
                    </t:Column>
                    <!-- Package Quantity -->
                    <t:Column id="_IDGenColumn13" width="8rem" sortProperty="PkgPcsVal" filterProperty="PkgPcsVal">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel13" text="{i18n>packageQty}" required="true"/>
                        <t:template>
                            <m:HBox id="_IDGenHBox3">
                                <m:Input id="packageQtyId13" value="{POTableModel>PkgPcsVal}" type="Number" required="true" editable="{= ${POTableModel>ReadOnly} !== 'X' }" />
                            </m:HBox>
                        </t:template>
                    </t:Column>
                    <!-- Package Type -->
                    <t:Column id="_IDGenColumn16" width="8rem">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel16" text="{i18n>packageType}" required="true"/>
                        <t:template>
                            <m:ComboBox id="comboBox_PkgPcsUni" selectedKey="{POTableModel>PkgPcsUni}" items="{path:'/ZI_PkgUnitOfMeasureVH',templateShareable:false}" showSecondaryValues="true" required="true" editable="{= ${POTableModel>ReadOnly} !== 'X' }">
                                <core:ListItem id="_IDGenListItem3" key="{UnitOfMeasure}" text="{UnitOfMeasure}" additionalText="{UnitOfMeasureLongName}" />
                            </m:ComboBox>
                        </t:template>
                    </t:Column>
                    <!-- Package Dimensions -->
                    <t:Column id="_IDGenColumn14" width="16rem" sortProperty="PkgLength" filterProperty="PkgLength">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel14" text="{i18n>dimensions}" />
                        <t:template >
                            <m:HBox id="_IDGenHBox1">
                                <m:Input id="lengthId" value="{POTableModel>PkgLength}" type="Number" editable="{= ${POTableModel>ReadOnly} !== 'X' }" required="true" />
                                <m:Input id="widthId" value="{POTableModel>PkgWidth}" type="Number" editable="{= ${POTableModel>ReadOnly} !== 'X' }" required="true" />
                                <m:Input id="heightId" value="{POTableModel>PkgHeight}" type="Number" editable="{= ${POTableModel>ReadOnly} !== 'X' }" required="true"/>
                                <m:ComboBox id="_IDGenComboBox14" value="{POTableModel>PkgMeasuom}" items="{path:'/ZI_DimUnitOfMeasureVH',templateShareable:false}" showSecondaryValues="true" editable="{= ${POTableModel>ReadOnly} !== 'X' }">
                                    <core:ListItem id="_IDGenListItem5" key="{UnitOfMeasure}" text="{UnitOfMeasure}" additionalText="{UnitOfMeasureLongName}" />
                                    <!-- <core:ListItem id="_IDGenListItem1" key="CM" text="CM" additionalText="Centimeter"/>
                                    <core:ListItem id="_IDGenListItem2" key="M" text="M" additionalText="Meter"/>
                                    <core:ListItem id="_IDGenListItem5" key="FT" text="FT" additionalText="Foot"/> -->
                                </m:ComboBox>
                            </m:HBox>
                        </t:template>
                    </t:Column>
                    <!-- Package Weight  -->
                    <t:Column id="_IDGenColumn15" width="10rem" sortProperty="PkgWeiVal" filterProperty="PkgWeiVal">
                        <t:customData>
                            <core:CustomData key="p13nData" value='{"columnKey": "PkgWeiVal" , "leadingProperty":"PkgWeiVal"}'/>
                        </t:customData>
                        <m:Label id="_IDGenLabel15" text="{i18n>weight}" />
                        <t:template>
                            <m:HBox id="_IDGenHBox15">
                                <m:Input id="weightId" value="{POTableModel>PkgWeiVal}" type="Number" editable="{= ${POTableModel>ReadOnly} !== 'X' }" />
                                <m:ComboBox id="_IDGenComboBox2" value="{POTableModel>PkgWeiUni}" showSecondaryValues="true" editable="{= ${POTableModel>ReadOnly} !== 'X' }">
                                    <core:ListItem id="_IDGenListItem2.1" key="LB" text="LB" additionalText="Pound"/>
                                    <core:ListItem id="_IDGenListItem2.2" key="G" text="G" additionalText="Gram"/>
                                    <core:ListItem id="_IDGenListItem2.3" key="KG" text="KG" additionalText="Kilogram"/>
                                </m:ComboBox>
                            </m:HBox>
                        </t:template>
                    </t:Column>
                    <!-- Package ID -->
                    <t:Column id="_IDGenColumn17" width="8rem" sortProperty="PkgId" filterProperty="PkgId">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel17" text="{i18n>packageid}" required="true"/>
                        <t:template>
                            <m:Input id="packMatInfoId17" value="{POTableModel>PkgId}" editable="{= ${POTableModel>ReadOnly} !== 'X' }" required="true"/>
                        </t:template>
                    </t:Column>
                    <!-- Pick up date -->
                    <t:Column id="_IDGenColumn18" width="12rem" sortProperty="PkgPickupDt" filterProperty="PkgPickupDt">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="idLabPickup18" text="{i18n>pickupdate}" required="true"/>
                        <t:template>
                            <m:DatePicker id="pickUpDateId" editable="{= ${POTableModel>ReadOnly} !== 'X' }" value="{path: 'POTableModel>PkgPickupDt',type: 'sap.ui.model.type.DateTime', formatOptions: {pattern: 'yyyy-MM-dd HH:mm'}}"/>
                        </t:template>
                    </t:Column>
                    <!-- Pick up Location -->
                    <t:Column id="_IDGenColumn19" width="15rem" sortProperty="PkgSrcLoc" filterProperty="PkgSrcLoc">
                        <t:customData>
                            <core:CustomData key="p13nData" />
                        </t:customData>
                        <m:Label id="_IDGenLabel19" text="{i18n>pickupLoc}" />
                        <t:template>
                            <m:Select id="_IDGenComboBox20" width="300px" selectedKey="{POTableModel>PkgSrcLoc}" editable="{= ${POTableModel>ReadOnly} !== 'X' }" items="{POTableModel>to_Shipper/results}">
                                <core:Item id="_IDGenListItem20" key="{POTableModel>Location}" text="{POTableModel>LocDescr}"></core:Item>
                            </m:Select>
                        </t:template>
                    </t:Column>
                    <!-- Reefer Comply -->
                    <t:Column id="_IDGenColumn21" width="8rem" sortProperty="PkgReeferComply" filterProperty="PkgReeferComply">
                        <t:customData>
                            <core:CustomData key="p13nData"></core:CustomData>
                        </t:customData>
                        <m:Label id="_IDGenLabel21" text="{i18n>reefComply}"/>
                        <t:template>
                            <m:CheckBox id="idreefComply" editable="{= ${POTableModel>ReadOnly} !== 'X' }" selected="{POTableModel>PkgReeferComply}" valueState="Information"></m:CheckBox>
                        </t:template>
                    </t:Column>
                </t:columns>
            </t:Table>
        </m:content>
        <m:footer>
            <m:Toolbar id="toolbar2">
                <m:content>
                    <m:ToolbarSpacer id="spacer1" />
                    <m:Button id="idPacktogether" text="Pack Item" type="Emphasized" press="onPackItmPress"/>
                    <m:Button press="onSubmitPress" text="Save" id="button4" type="Emphasized" />
                </m:content>
            </m:Toolbar>
        </m:footer>
    </m:Page>
</mvc:View>